            async function testAPI() {
                try {
                    const response = await fetch('/test-api');
                    const data = await response.json();
                    if (data.detectedRegion) {
                        alert('APIÊ£ÄÊµãÁªìÊûú: ' + data.detectedRegion + '\\nÊ£ÄÊµãÊó∂Èó¥: ' + data.timestamp);

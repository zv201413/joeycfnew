                        }
                        geoInfo.innerHTML = 'Ê£ÄÊµãÊñπÂºè: <span style="color: #44ff44;">CloudflareÂÜÖÁΩÆÊ£ÄÊµã</span>';
                    } catch (e) {
                        geoInfo.innerHTML = 'Ê£ÄÊµãÊñπÂºè: <span style="color: #ff4444;">Ê£ÄÊµãÂ§±Ë¥•</span>';
                    }
                    regionStatus.innerHTML = 'WorkerÂú∞Âå∫: <span style="color: #44ff44;">‚úÖ ' + regionNames[detectedRegion] + '</span>';

                        alert('APIÊ£ÄÊµãÂ§±Ë¥•: ' + (data.error || 'Êú™Áü•ÈîôËØØ'));
                    }
                } catch (error) {
                    console.error('APIÊµãËØïÂ§±Ë¥•:', error);
                    alert('APIÊµãËØïÂ§±Ë¥•: ' + error.message);
                }
            }
            async function checkKVStatus() {
                const apiUrl = window.location.pathname + atob('L2FwaS9jb25maWc=');
                console.log('üîç Ë∞ÉËØï‰ø°ÊÅØ - Ê£ÄÊü•KVÁä∂ÊÄÅ');
                console.log('ËØ∑Ê±ÇURL:', apiUrl);
                try {
                    const response = await fetch(apiUrl);
                    console.log('ÂìçÂ∫îÁä∂ÊÄÅ:', response.status);
                    console.log('ÂìçÂ∫îÂ§¥:', Object.fromEntries(response.headers.entries()));
                    if (response.status === 503) {
                        console.log('‚ùå KVÂ≠òÂÇ®Êú™ÈÖçÁΩÆ (503)');
                        document.getElementById('kvStatus').innerHTML = '<span style="color: #ffaa00;">‚ö†Ô∏è KVÂ≠òÂÇ®Êú™ÂêØÁî®ÊàñÊú™ÈÖçÁΩÆ</span>';
                        document.getElementById('configCard').style.display = 'block';
                        document.getElementById('currentConfig').textContent = 'KVÂ≠òÂÇ®Êú™ÈÖçÁΩÆÔºåÊó†Ê≥ï‰ΩøÁî®ÈÖçÁΩÆÁÆ°ÁêÜÂäüËÉΩ„ÄÇ\\n\\nËØ∑Âú®Cloudflare Workers‰∏≠:\\n1. ÂàõÂª∫KVÂëΩÂêçÁ©∫Èó¥\\n2. ÁªëÂÆöÁéØÂ¢ÉÂèòÈáè C\\n3. ÈáçÊñ∞ÈÉ®ÁΩ≤‰ª£Á†Å';
                    } else if (response.ok) {
                        const data = await response.json();
                        console.log('ÂìçÂ∫îÊï∞ÊçÆ:', data);
                        console.log('kvEnabled:', data.kvEnabled);
                        if (data && data.kvEnabled === true) {
                            console.log('‚úÖ KVÂ≠òÂÇ®Â∑≤ÂêØÁî®');
                            document.getElementById('kvStatus').innerHTML = '<span style="color: #44ff44;">‚úÖ KVÂ≠òÂÇ®Â∑≤ÂêØÁî®ÔºåÂèØ‰ª•‰ΩøÁî®ÈÖçÁΩÆÁÆ°ÁêÜÂäüËÉΩ</span>';
                            document.getElementById('configContent').style.display = 'block';
                            document.getElementById('configCard').style.display = 'block';
                            await loadCurrentConfig();
                        } else {
                            console.log('‚ùå KVÂ≠òÂÇ®Êú™ÂêØÁî® (ÂìçÂ∫î‰∏≠kvEnabled‰∏ç‰∏∫true)');
                            document.getElementById('kvStatus').innerHTML = '<span style="color: #ffaa00;">‚ö†Ô∏è KVÂ≠òÂÇ®Êú™ÂêØÁî®ÊàñÊú™ÈÖçÁΩÆ</span>';
                            document.getElementById('configCard').style.display = 'block';
                            document.getElementById('currentConfig').textContent = 'KVÂ≠òÂÇ®Êú™ÈÖçÁΩÆ';
                        }
                    } else {
                        console.log('‚ùå ÂìçÂ∫îÈîôËØØ:', response.status, response.statusText);
                        document.getElementById('kvStatus').innerHTML = '<span style="color: #ffaa00;">‚ö†Ô∏è KVÂ≠òÂÇ®Êú™ÂêØÁî®ÊàñÊú™ÈÖçÁΩÆ</span>';
                        document.getElementById('configCard').style.display = 'block';
                        document.getElementById('currentConfig').textContent = 'KVÂ≠òÂÇ®Ê£ÄÊµãÂ§±Ë¥• - Áä∂ÊÄÅÁ†Å: ' + response.status;
                    }
                } catch (error) {
                    console.log('‚ùå ËØ∑Ê±ÇÂ§±Ë¥•:', error);
                    document.getElementById('kvStatus').innerHTML = '<span style="color: #ffaa00;">‚ö†Ô∏è KVÂ≠òÂÇ®Êú™ÂêØÁî®ÊàñÊú™ÈÖçÁΩÆ</span>';
                    document.getElementById('configCard').style.display = 'block';
                    document.getElementById('currentConfig').textContent = 'KVÂ≠òÂÇ®Ê£ÄÊµãÂ§±Ë¥• - ÈîôËØØ: ' + error.message;
                }
            }
            async function loadCurrentConfig() {
                const apiUrl = window.location.pathname + atob('L2FwaS9jb25maWc=');
                console.log('üì• Ë∞ÉËØï‰ø°ÊÅØ - Âä†ËΩΩÈÖçÁΩÆ');
                console.log('ËØ∑Ê±ÇURL:', apiUrl);
                try {
                    const response = await fetch(apiUrl);
                    console.log('Âä†ËΩΩÂìçÂ∫îÁä∂ÊÄÅ:', response.status);
                    if (response.status === 503) {
                        console.log('‚ùå KVÂ≠òÂÇ®Êú™ÈÖçÁΩÆÔºåÊó†Ê≥ïÂä†ËΩΩÈÖçÁΩÆ');
                        document.getElementById('currentConfig').textContent = 'KVÂ≠òÂÇ®Êú™ÈÖçÁΩÆÔºåÊó†Ê≥ïÂä†ËΩΩÈÖçÁΩÆ';
                        return;
                    }
                    if (!response.ok) {
                        console.log('‚ùå Âä†ËΩΩÈÖçÁΩÆÂ§±Ë¥•ÔºåÁä∂ÊÄÅÁ†Å:', response.status);
                        document.getElementById('currentConfig').textContent = 'Âä†ËΩΩÈÖçÁΩÆÂ§±Ë¥•';
                        return;
                    }
                    const config = await response.json();
                    console.log('Âä†ËΩΩÁöÑÈÖçÁΩÆÊï∞ÊçÆ:', config);
                    const displayConfig = {};
                    for (const [key, value] of Object.entries(config)) {
                        if (key !== 'kvEnabled') {
                            displayConfig[key] = value;
                        }
                    }
                    let configText = 'ÂΩìÂâçÈÖçÁΩÆ:\\n';
                    if (Object.keys(displayConfig).length === 0) {
                        configText += '(ÊöÇÊó†ÈÖçÁΩÆ)';
                    } else {
                        for (const [key, value] of Object.entries(displayConfig)) {
                            configText += key + ': ' + (value || '(Êú™ËÆæÁΩÆ)') + '\\n';
                        }
                    }
                    document.getElementById('currentConfig').textContent = configText;
                    document.getElementById('wkRegion').value = config.wk || '';
                    document.getElementById('customIP').value = config.p || '';
                    document.getElementById('preferredIPs').value = config.yx || '';
                    document.getElementById('preferredIPsURL').value = config.yxURL || '';
                    document.getElementById('socksConfig').value = config.s || '';
                    document.getElementById('apiEnabled').value = config.apiEnabled || '';
                    document.getElementById('regionMatching').value = config.rm || '';
                document.getElementById('downgradeControl').value = config.qj || '';
                document.getElementById('portControl').value = config.dkby || '';
                document.getElementById('preferredControl').value = config.yxby || '';
                updateWkRegionState();
            } catch (error) {
                document.getElementById('currentConfig').textContent = 'Âä†ËΩΩÈÖçÁΩÆÂ§±Ë¥•: ' + error.message;
            }
        }
        function updateWkRegionState() {
            const customIP = document.getElementById('customIP');
            const wkRegion = document.getElementById('wkRegion');
            const wkRegionHint = document.getElementById('wkRegionHint');
            if (customIP && wkRegion) {
                const hasCustomIP = customIP.value.trim() !== '';
                wkRegion.disabled = hasCustomIP;
                if (hasCustomIP) {
                    wkRegion.style.opacity = '0.5';
                    wkRegion.style.cursor = 'not-allowed';
                    wkRegion.style.backgroundColor = 'rgba(0, 0, 0, 0.5)';
                    if (wkRegionHint) {
                        wkRegionHint.style.display = 'block';
                        wkRegionHint.style.color = '#ffaa00';
                    }
                } else {
                    wkRegion.style.opacity = '1';
                    wkRegion.style.cursor = 'pointer';
                    wkRegion.style.backgroundColor = 'rgba(0, 0, 0, 0.8)';
                    if (wkRegionHint) {
                        wkRegionHint.style.display = 'none';
                    }
                }
            }
        }
        async function saveConfig(configData) {
                const apiUrl = window.location.pathname + atob('L2FwaS9jb25maWc=');
                console.log('üíæ Ë∞ÉËØï‰ø°ÊÅØ - ‰øùÂ≠òÈÖçÁΩÆ');
                console.log('ËØ∑Ê±ÇURL:', apiUrl);
                console.log('ÈÖçÁΩÆÊï∞ÊçÆ:', configData);
                try {
                    const response = await fetch(apiUrl, {
                        method: 'POST',
                        headers: { 'Content-Type': 'application/json' },
                        body: JSON.stringify(configData)
                    });
                    console.log('‰øùÂ≠òÂìçÂ∫îÁä∂ÊÄÅ:', response.status);
                    console.log('‰øùÂ≠òÂìçÂ∫îÂ§¥:', Object.fromEntries(response.headers.entries()));
                    if (response.status === 503) {
                        console.log('‚ùå KVÂ≠òÂÇ®Êú™ÈÖçÁΩÆÔºåÊó†Ê≥ï‰øùÂ≠ò');
                        showStatus('KVÂ≠òÂÇ®Êú™ÈÖçÁΩÆÔºåÊó†Ê≥ï‰øùÂ≠òÈÖçÁΩÆ„ÄÇËØ∑ÂÖàÂú®Cloudflare Workers‰∏≠ÈÖçÁΩÆKVÂ≠òÂÇ®„ÄÇ', 'error');
                        return;
                    }
                    const result = await response.json();
                    console.log('‰øùÂ≠òÂìçÂ∫îÊï∞ÊçÆ:', result);
                    showStatus(result.message, result.success ? 'success' : 'error');
                    if (result.success) {
                        console.log('‚úÖ ‰øùÂ≠òÊàêÂäüÔºåÈáçÊñ∞Âä†ËΩΩÈÖçÁΩÆ');
                        await loadCurrentConfig();
                        setTimeout(function() {
                            window.location.reload();
                        }, 1500);
                    } else {
                        console.log('‚ùå ‰øùÂ≠òÂ§±Ë¥•:', result.message);
                    }
                } catch (error) {
                    console.log('‚ùå ‰øùÂ≠òËØ∑Ê±ÇÂ§±Ë¥•:', error);
                    showStatus('‰øùÂ≠òÂ§±Ë¥•: ' + error.message, 'error');
                }
            }
            function showStatus(message, type) {
                const statusDiv = document.getElementById('statusMessage');
                statusDiv.textContent = message;
                statusDiv.style.display = 'block';
                statusDiv.style.color = type === 'success' ? '#00ff00' : '#ff0000';
                statusDiv.style.borderColor = type === 'success' ? '#00ff00' : '#ff0000';
                setTimeout(function() {
                    statusDiv.style.display = 'none';
                }, 3000);
            }
